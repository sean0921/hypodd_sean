1.2
log
@Add RCS identification string.
@
text
@d1 2
d4 12
a15 6
      subroutine datetime(dattim)       ! Urs Kradolfer, 16.9.91
c     File:    datetime_sun.f
c     return UNIX-date and time
c     Call:    call DATETIME(dattim)
c     returned value: dattim  is a character*20
      implicit none
d18 1
a18 1
	data rcsid /"$Header: /home/crhet/julian/src/cmd/hypoDD/RCS/datetime.f,v 1.2 2001/01/25 23:46:12 julian Exp julian $"/
a20 2
      character datum*26, dattim*20
      integer it, itime, ctime, time
a30 25

      subroutine covar(maxeve0,v,n,q,cvm)
      implicit none
      integer*4 maxeve0,n,i,j,k
      real v(maxeve0*4,maxeve0*4),q(maxeve0*4),
     &     cvm(maxeve0*4,maxeve0*4), sum
c     &     wti(maxeve0*4)

c      do i=1,n
c         wti(i)= 0
c         if(q(i).ne.0) wti(i)= 1/(q(i)*q(i))
c      enddo
      do i=1,n
         do j=1,i
           sum= 0
           do k=1,n
              if(q(k).ne.0) sum= sum + v(i,k)*v(j,k) * (1/(q(k)*q(k)))
c              sum= sum + v(i,k)*v(j,k)*wti(k)
           enddo
           cvm(i,j)= sum
           cvm(j,i)= sum
         enddo
      enddo
      return
      end
@


